@charset "utf-8";
/*========== 공통 ==========*/
/* 레이아웃 */
#wrap{height:100%}
.area, #header .area > div{width:90%;margin:0 auto;}
#header{display:table;width:100%;height:40px;line-height:40px;background-color:#0ab4ed}
#header .area .t_l{text-align:left}
#header .area .t_r{text-align:right}
#header .area > div{position:relative;width:100%;}
#header .tit{text-align:center;font-size:1.25em;color:#fff}
#content .area form{padding-top:3.301127214170692%;}

/* 버튼 */
button[type="button"].btn_style2, .btn_style2{position:absolute;background-repeat:no-repeat;background-position:0 3px;-webkit-background-size:contain;-moz-background-size:contain;-o-background-size:contain;background-size:contain;text-indent:-9999px;}
button[type="button"].btn_back, a.btn_back, .btn_back{top:50%;margin-top:-10px;left:0;width:20px;height:20px;background-image:url("../images/icon/back.png")}
button[type="button"].btn_menu, button[type="button"].btn_setup{top:50%;width:20px;-webkit-background-size:contain;-moz-background-size:contain;-o-background-size:contain;background-size:contain}
button[type="button"].btn_menu{left:0;height:20px;margin-top:-10px;background-image:url("../images/icon/menu.png")}
button[type="button"].btn_setup{right:0;height:24px;margin-top:-12px;background-image:url("../images/icon/setup.png")}
button[type="button"].btn_inquery{background-color:#09b6ee}
button[type="button"].btn_search{width:100%;height:35px;margin-left:10px;background-color:#09b6ee;background-image:url("../images/icon/search.png");background-position:50% 50%;-webkit-background-size:20px 20px;-moz-background-size:20px 20px;-o-background-size:20px 20px;background-size:20px 20px;-webkit-border-radius:50px;-moz-border-radius:50px;border-radius:50px;}

/* 버튼 선택(select) */
.btn_select{width:100%;border:1px solid #14a6e7;-webkit-border-radius:50px;-moz-border-radius:50px;border-radius:50px;}
.btn_select button[type="button"]:first-child{-webkit-border-radius:50px 0 0 50px;-moz-border-radius:50px 0 0 50px;border-radius:50px 0 0 50px}
.btn_select button[type="button"]:nth-child(2){-webkit-border-radius:0 50px 50px 0;-moz-border-radius:0 50px 50px 0;border-radius:0 50px 50px 0}
.btn_select button[type="button"]{float:left;width:50%;padding:9.5px 0;color:#8e8e8e;background-color:#e3e3e3;}
.btn_select button[type="button"].on{color:#fff;background-color:#0ab4ed;}

/* 버튼 끄고 켜기(알람) */	
#content .switch {display:inline-block;position:relative;top:0;left:0;margin-top:0;width:50px;height: 26px;}
.switch input {display:none;}
.slider {position:absolute;top:0;left:0;right:0;bottom:0;background-color:#ccc;-webkit-transition:0.4s;transition:0.4s;}
.slider:before{position: absolute;content:"";height:20px;width:20px;left:3px;bottom:3px;background-color:#fff;-webkit-transition:0.4s;transition:0.4s;}
input:checked + .slider{background-color: #09b6ee;}
input:focus + .slider{box-shadow:0 0 1px #09b6ee;}
input:checked + .slider:before{background-image:url("../images/icon/check_on.png");background-repeat:no-repeat;background-position:50% 50%;-webkit-background-size:60% 40%;-moz-background-size:60% 40%;-o-background-size:60% 40%;background-size:60% 40%;-webkit-transform: translateX(24px);-ms-transform: translateX(24px);transform: translateX(24px);}
.slider.round{-webkit-border-radius:34px;-moz-border-radius:34px;border-radius:34px;}
.slider.round:before{-webkit-border-radius:50%;-moz-border-radius:50%;border-radius:50%;}

/* input */
#content{color:#7b7b7b}
#content form p{position:relative;margin-bottom:3.301476976542137%}
#content form p.t_p{margin:-3.301476976542137% 0 0 0;padding:1.170411985018727% 0 1.49812734082397% 5.994787141615986%;font-size:0.813em}
#content input, #content select{width:100%;height:35px;line-height:35px;padding:0 12% 0 5%;border:1px solid #c5c5c5;text-align:right;-webkit-appearance:none;-moz-appearance:none;appearance: none;}
#content select{direction:rtl}/* 텍스트 오른쪽 정렬 */
#content input:focus, #content select:focus{border:2px solid #14a6e7}
#content label{position:absolute;top:50%;margin-top:-8px;left:25px;font-size:0.875em}
#content .calendar, #content .time{padding-left:20%;font-size:0.9em;color:#7b7b7b;text-align:center}
#content .calendar{padding-left:23%}

#content .wid:after{display:block;content:"";clear:both}
#content .wid p, #content .wid div{float:left}
#content .wid p:last-child, #content .wid div{padding-left:10px}
#content .wid p.btn_select{padding-left:0}
#content .wid10{width:10%}
#content .wid20{width:20%}
#content .wid30{width:30%}
#content .wid40{width:40%}
#content .wid50{width:50%}
#content .wid60{width:60%}
#content .wid70{width:70%}
#content .wid80{width:80%}
#content .wid90{width:90%}

/* label */
#content label.label_date, #content label.label_time{margin-top:-10px;background-repeat:no-repeat;-webkit-background-size:contain;-moz-background-size:contain;-o-background-size:contain;background-size:contain;text-indent:-9999px;}
#content label.label_date{width:20px;left:13px;padding-top:calc(20/63*100%);background-image:url("../images/icon/date.png")}
#content label.label_time{width:20px;left:20px;padding-top:calc(20/69*100%);background-image:url("../images/icon/time.png")}

/* 메뉴 영역 */
#gnb, #snb, .over_bg{position:absolute;top:0;}
#gnb, #snb{display:none;height:100%;overflow-y:auto;overflow-x:hidden;background-color:#fff;z-index:1;}
#gnb .user_info span, #snb .setup span{margin-top:7px;padding-left:10px;}
#gnb .close_btn, #snb .close_btn{width:20px;height:20px;margin-top:5px;background-repeat:no-repeat;text-indent:-9999px;background-size:100% 100%}
#gnb .close_btn{background-image:url("../images/button/close.png")}
#snb .close_btn{background-image:url("../images/button/close_gray.png")}

#gnb{width:85%;left:-68%;overflow-y:auto;overflow-x:hidden}
.user_info{width:100%;padding:10px 0;background:#0ab4ed}
.user_info img{width:30px;height:30px;vertical-align:middle}
#gnb .user_info span{color:#fff}
.home a{display:block;height:35px;line-height:35px;padding-left:28px;color:#575757;font-size:0.875em;background:url("../images/icon/home.png") no-repeat 0 50%;-webkit-background-size:20px 20px;-moz-background-size:20px 20px;-o-background-size:20px 20px;background-size:20px 20px}

#gnb .depth1 p{padding:7px 5%;background-color:#ededed;border:1px solid #dbdbdb;border-left:0;border-right:0;color:#575757;font-size:0.875em;}
#gnb .depth2:after{display:block;content:'';clear:both}
#gnb .depth2 li{float:left;width:33.333333333%;height:80px;border-right:1px solid #dbdbdb;background-repeat:no-repeat;background-position:50% 32%;background-size:32px}/* depth2 공통 */
#gnb .depth2 li:nth-child(3){border-right:0}
#gnb .depth1:last-child{border-bottom:1px solid #dbdbdb;}

#gnb .depth1:first-child li:first-child{background-image:url("../images/icon/bg_bt_mst.png");background-size:20px}/* 혈당 | 측정 */
#gnb .depth1:first-child li:nth-child(2), #gnb .depth1:nth-child(2) li:nth-child(2), #gnb .depth1:nth-child(3) li:nth-child(2){background-image:url("../images/icon/manual.png")}
#gnb .depth1:first-child li:nth-child(3), #gnb .depth1:nth-child(2) li:nth-child(3), #gnb .depth1:nth-child(3) li:nth-child(3), #gnb .depth1:nth-child(4) li:first-child, #gnb .depth1:nth-child(5) li:first-child{background-image:url("../images/icon/report.png");background-position:50% 28%;}
#gnb .depth1:nth-child(2) li:first-child{background-image:url("../images/icon/bp_bt_mst.png");background-size:30px}
#gnb .depth1:nth-child(3) li:first-child{background-image:url("../images/icon/bmi_bt_mst.png");background-size:25px}

#gnb .depth2 li a{display:flex;align-items:center;justify-content:center;height:28px;padding-top:42px;color:#575757;text-align:center;font-size:0.875em;letter-spacing:-1px}

#snb{width:85%;right:-85%;}
.setup{width:85%;margin:0 20px;padding:20px 0}
.setup img{width:20px;height:20px;vertical-align:sub}
#snb .setup span{display:inline-block;margin-top:7px;padding-left:10px;color:#494e65}
#snb nav > ul > li > a{display:block;margin-left:42px;padding:15px 0;color:#494e65;font-size:0.875em;background:url("../images/icon/clamp.png") no-repeat 92% 50%;background-size:8px 13px}
#snb nav > ul > li > a{border-top:1px solid #ccc}
#snb nav > ul > li:last-child > a, #snb nav > ul > li.last > a{border-bottom:1px solid #ccc}

.over_bg{left:0;width:100%;height:100%;background:url("../images/bg/over64_1x1.png") repeat;}

/* 달력, 시계 */
.dwb#dw_cancel, .dwb#dw_set{background-image:none;box-shadow:none;text-shadow:none;}
.dwb#dw_cancel{background-color:#9c9c9c}/* 취소 */
.dwb#dw_set{background-color:#09b6ee}/* 선택 */

.dw{width:234px}

.mbsc-comp{color:#7b7b7b}/* input color */

/* 사용자등록 */
.user_inquery #content .area form{padding-top:3.301127214170692%}
#footer{margin:6% 0 5%}
#footer p{line-height:20px;padding-bottom:2.4812734082397%;text-align:center;color:#7b7b7b;font-size:0.875em}
#footer p a{color:#000}

/* 171101 로딩 중 처리 */
.square{position:absolute;top:50%;left:50%;width:70px;height:70px;padding:0px;margin:-35px 0 0 -35px;border:0;}
.spin {width:70px;height:70px;border-radius: 50%;border:dashed 5px white;
  -webkit-animation-name: spin;
  -webkit-animation-duration: 2s;
  -webkit-animation-iteration-count: infinite;
  -webkit-animation-timing-function: linear;
}

@-webkit-keyframes spin {
  from   {  -webkit-transform: rotate(0deg); }
  to   {  -webkit-transform: rotate(360deg); }
}

/*========== 최근 측정 정보(메인) ==========*/
/* 상태 color (공통) */
.danger .figure, .danger.figure{background-color:#e01e30}
.warning .figure, .warning.figure{background-color:#f14555}
.high .figure, .high.figure{background-color:#ff8f29}
.normal .figure, .normal.figure{background-color:#0ab4ed}
.low .figure, .low.figure{background-color:#fdc82e}
.none .figure, .none.figure{background-color:#d2d2d2}

.bg_info .day_info .danger + .f_l p:first-child, .bp_info .day_info .danger p:nth-child(2){color:#e01e30}
.bg_info .day_info .warning + .f_l p:first-child, .bp_info .day_info .warning p:nth-child(2){color:#f14555}
.bg_info .day_info .high + .f_l p:first-child, .bp_info .day_info .high p:nth-child(2){color:#ff8f29}
.bg_info .day_info .normal + .f_l p:first-child, .bp_info .day_info .normal p:nth-child(2){color:#0ab4ed}
.bg_info .day_info .low + .f_l p:first-child, .bp_info .day_info .low p:nth-child(2){color:#fdc82e}

/* 최근 측정 정보 공통 */
.main #content{padding-bottom:10px;background-color:#eee}
.main #content h2.tit{height:40px;line-height:40px;background:#0ab4ed;color:#fff;text-align:center}
.main #content h2.tit img{width:20px;margin-right:5px;vertical-align:middle}
.new_info > div{margin-top:10px;border:1px solid #d0d1d9;-webkit-border-radius:5px;-moz-border-radius:5px;-o-border-radius:5px;border-radius:5px}
.new_info > div > div:nth-child(1){height:30px;line-height:30px;padding:0 5%;background:#d0d1d9;-webkit-border-radius:4px 4px 0 0;-moz-border-radius:4px 4px 0 0;-o-border-radius:4px 4px 0 0;border-radius:4px 4px 0 0}
.new_info .tit{color:#464854}
.new_info .now_date{color:#717381;font-size:0.9em}
.new_info > div > div:nth-child(2){padding:10px 5%;background-color:#fff;-webkit-border-radius:0 0 5px 5px;-moz-border-radius:0 0 5px 5px;-o-border-radius:0 0 5px 5px;border-radius:0 0 5px 5px}

.day_info .figure{width:75px;height:75px;-webkit-border-radius:50%;-moz-border-radius:50%;-o-border-radius:50%;border-radius:50%;color:#fff}
.day_info .figure p{text-align:center}

.week_info .list li{padding:3% 7px;border-bottom:1px solid #d6d6d6}
