.week_info .list li p{text-align:center;font-size:0.75em;}
.week_info .list li p:first-child{float:left}
.week_info .list li p:last-child{float:right}

button[type=button].input_btn{position:absolute;right:0;bottom:2.867072111207646%;width:40px;padding-top:calc(10/136*100%);background:url("../images/button/manual_input.png") no-repeat;-webkit-background-size:contain;-moz-background-size:contain;-o-background-size:contain;background-size:contain;text-indent:-9999px}

/* 혈당 - 최근 측정 정보 */
.bg_info .day_info{background:url("../images/bg/main_bg.jpg") repeat-x 0 50%;-webkit-background-size:1px;-moz-background-size:1px;-o-background-size:1px;background-size:1px;letter-spacing:-1px}
.bg_info .day_info .clear{margin:0 auto;padding:0 7px;background-color:#fff;} 
.bg_info .day_info .f_l:first-child{padding-top:10px;}
.bg_info .day_info .f_l:first-child p:first-child{font-size:2.5em;letter-spacing:-2px;}
.bg_info .day_info .f_l:first-child p:nth-child(2), .bg_info .day_info .f_l:nth-child(2) p:nth-child(2), .bp_info .day_info .figure p:nth-child(1), .bp_info .day_info .figure p:nth-child(3){font-size:0.8em;letter-spacing:-1px;}

.bg_info .day_info .f_l:nth-child(2){padding:13px 0 13px 10px}
.bg_info .day_info .f_l:nth-child(2) p:first-child{padding-bottom:2px;font-size:1.7em;letter-spacing:-2px}

.week_info h4{margin:10px 0;color:#555;letter-spacing:-1px}
