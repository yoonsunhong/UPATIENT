/* 혈압 - 최근 측정 정보 */
.bp_info .day_info p:nth-child(1){color:#000;font-size:1.1em;letter-spacing:-1px;}
.bp_info .day_info p:nth-child(2){padding:3px 0 5px;font-size:2em;letter-spacing:-3px;}
