.bg_info .week_info .f_l{width:40%}
.bg_info .week_info > .clear .f_l > div + div{position:absolute;top:40%;left:0;width:100%}
.bg_info .week_info > .clear .f_l p{color:#6f6f6f;font-size:0.8em;text-align:center;letter-spacing:-1px;}
.bg_info .week_info > .clear .f_l p + p{color:#3f464a;font-size:1.5em;text-align:center;letter-spacing:-3px;}

.bg_info .week_info .list{position:absolute;top:50%;right:0;width:60%;-webkit-transform: translateY(-50%);-moz-transform: translateY(-50%);-ms-transform: translateY(-50%);-o-transform: translateY(-50%);transform: translateY(-50%);}
.bg_info .week_info .list ul{padding-left:10%;background:url("../images/bg/chart1_label.png") no-repeat 5% 50%;-webkit-background-size:7px 80%;-moz-background-size:7px 80%;-o-background-size:7px 80%;background-size:7px 80%}
