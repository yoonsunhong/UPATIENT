.bp_info .week_info h5{padding:3% 0;background-color:#f5f5f5;border-top:1px solid #d6d6d6;text-align:center;font-size:0.8em;}
.bp_info .week_info .list li p:nth-child(2){float:left;width:calc( 100% - 27px );text-align:left}
.week_info .list li p:last-child{width:13px;text-align:center;color:#151515}

/* BMI, 활동 - 최근 측정 정보 */
