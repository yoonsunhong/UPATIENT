.bmi_info .day_info p:nth-child(2){width:55px;padding:0 calc((100% - 200px)/4)}
.bmi_info .day_info p:nth-child(3){width:78px;padding-left:calc((100% - 200px)/4)}

/* 활동 - 최근 측정 정보 */
