.bp_info .day_info .figure, .bmi_info .day_info .figure{width:100%;height:35px;line-height:35px;font-size:1.2em;text-align:center;-webkit-border-radius:7px;-moz-border-radius:7px;-o-border-radius:7px;border-radius:7px;}
    
.bp_info .week_info .figure{width:7px;height:7px;margin:2% 7px 0 0;-webkit-border-radius:50%;-moz-border-radius:50%;-o-border-radius:50%;border-radius:50%}
