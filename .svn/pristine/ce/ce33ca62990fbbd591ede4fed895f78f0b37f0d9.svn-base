@charset "utf-8";
@import url(http://fonts.googleapis.com/earlyaccess/nanumgothic.css);

/* 초기화 */
html, body, div, span, h1, h2, h3, h4, h5, h6, p, img, em, strong, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, 
article, aside, figcaption, figure, footer, header, nav, section{margin:0;padding:0;border:0;font-size:1em;font-weight:normal;vertical-align:baseline;background:transparent;cursor:default;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;}
html, body{width:100%;height:100%;overflow-x:hidden;overflow-y:inherit}
body{/*font-family:"Nanum Gothic",나눔고딕, Sans-serif, Helvetica;*/line-height:1}
article, aside, figcaption, figure, footer, header, nav, section{display:block}
h1, h2, h3, h4, h5, h6{font-weight:normal}
li{list-style:none}
a{margin:0;padding:0;text-decoration:none;vertical-align:baseline;color:#fff;background:transparent;cursor:default}
img{vertical-align:top}
table{border-collapse:collapse;border-spacing:0}
input[type="text"], input[type="password"], input[type="number"], input[type="email"], select{margin:0;padding:10px 0;width:100%;height:34px;line-height:1;vertical-align:top;appearance:none;border:0;background-color:#fff;-webkit-border-radius:80050px;-moz-border-radius:50px;border-radius:50px;/*font-family:"Nanum Gothic",나눔고딕, Sans-serif, Helvetica;*/-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;}
input:hover, input:focus, button[type="button"]:hover, button[type="button"]:focus, select:hover, select:focus{outline:none}
input[type=text]:focus{border:1px solid #095088}
button[type="button"]{margin:0;padding:0;border:0;background:none}
input::-moz-input-placeholder{color:#afafaf}/* input placeholder color */
input::-webkit-input-placeholder{color:#afafaf}/* input placeholder color */
select{-webkit-appearance:none;-moz-appearance:none;appearance:none;background:url("../images/icon/select_arrow.png") no-repeat 95% 50%;background-size:10px 7px}/* 화살표 없애기*/
select::-ms-expand{display: none} /* 화살표 없애기 for IE10, 11*/
.blind{display:block;width:0px;height:0px;text-indent:-9999px}

/* 공통 클래스 */
.f_l{float:left}
.f_r{float:right}
.clear:after{display:block;content:"";clear:both}
.p_r{position:relative}
.p_a{position:absolute}

/* 공통 버튼 */
button[type="button"].btn_style1{width:100%;height:3.5%;min-height:34px;line-height:1;padding:10px 0;background:#fff;color:#fff;-webkit-border-radius:50px;-moz-border-radius:50px;border-radius:50px;}/* 둥근 모양의 버튼 */



