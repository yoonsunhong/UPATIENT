@charset "utf-8";
/*========== 공통 ==========*/
/* 레이아웃 */
#wrap{height:100%}
.area, #header .area > div{width:90%;margin:0px auto;}
#header{display:table;width:100%;height:40px;line-height:40px;background-color:#0ab4ed}
#header .area .t_l{text-align:left}
#header .area .t_r{text-align:right}
#header .area > div{position:relative;width:100%;}
#header .tit{text-align:center;font-size:1.25em;color:#fff}
